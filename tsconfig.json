{
	"compilerOptions": {
		"target": "ES2020",
		"module": "NodeNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"outDir": "dist",
		"moduleResolution": "nodenext"
	},
		"include": ["server/**/*.ts", "apps/**/*.ts", "packages/**/*.ts", "types/**/*.d.ts"]
}
