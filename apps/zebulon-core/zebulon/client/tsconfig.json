{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
  "module": "ES2020",
  "downlevelIteration": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "types": ["vite/client", "react", "react-dom"]
  ,"baseUrl": "src",
    "paths": {
      "@/*": ["./*"],
      "@shared/*": ["shared/*"]
    }
  },
  "include": ["src"]
}
